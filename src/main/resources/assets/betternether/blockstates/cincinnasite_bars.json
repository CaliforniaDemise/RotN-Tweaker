{
    "multipart": [
        {   "apply": { "model": "rotntweaker:cincinnasite_bars_post" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "rotntweaker:cincinnasite_bars_post", "uvlock": true }
        },
        {   "when": { "north": true },
            "apply": { "model": "rotntweaker:cincinnasite_bars_side", "uvlock": true }
        },
        {   "when": { "east": true },
            "apply": { "model": "rotntweaker:cincinnasite_bars_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "rotntweaker:cincinnasite_bars_side_alt", "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "rotntweaker:cincinnasite_bars_side_alt", "y": 90, "uvlock": true }
        }
    ]
}
